<!doctype html>

<html>
    <head>
      <% include ../partials/head %>
    </head>
    <body>
      <div class="outerContainer">
        <div class="header">
          <% include ../partials/header %>
        </div>
        <div class="showCaseWrapper">
          <div class="showCase w3-content w3-display-container" style="max-width:800px">
            <img class="mySlides" src="images/slide1.jpg" style="width: 100%">
            <img class="mySlides" src="images/slide2.jpg" style="width: 100%">
            <img class="mySlides" src="images/slide3.jpg" style="width: 100%">
            <div class="w3-center w3-container w3-section w3-large w3-text-white w3-display-bottommiddle" style="width: 100%">
              <div class="w3-left w3-hover-text-khaki" onclick="plusDivs(-1)">&#10094;</div>
              <div class="w3-right w3-hover-text-khaki" onclick="plusDivs(1)">&#10095;</div>
              <span class="w3-badge demo w3-border w3-transparent w3-hover-white" onclick="currentDiv(1)"></span>
              <span class="w3-badge demo w3-border w3-transparent w3-hover-white" onclick="currentDiv(2)"></span>
              <span class="w3-badge demo w3-border w3-transparent w3-hover-white" onclick="currentDiv(3)"></span>
            </div>
          </div>
        </div>
        <div class="contentMain">
          <div class="fContentWrapper">
            <div class="featuredContent"></div>
            <div class="featuredContent"></div>
            <div class="featuredContent"></div>
          </div>
          <div class="fContentWrapper">
            <div class="featuredContent"></div>
            <div class="featuredContent"></div>
          </div>
        </div>
        <div class="footer">
          <% include ../partials/footer %>
        </div>
      </div>
      <script type="text/javascript" src="javascripts/slideshow.js"></script>
    </body>
</html>
