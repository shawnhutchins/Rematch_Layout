<!doctype html>

<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <% include ../partials/head %>
  </head>
  <body>
    <div class="outerContainer">
      <div class="header">
        <% include ../partials/header %>
      </div>
      <div class="contentMain">
        <div class="streamMainWrapper">
          <div class="streamTwitchWrapper">
            <iframe class="streamTwitch"
              src="https://player.twitch.tv/?channel=rematch_gaming" 
              frameborder="0" 
              allowfullscreen="true" 
              scrolling="no">
            </iframe>
          </div>
          <div class="streamChatWrapper">
            <iframe class="streamTwitchChat"
              src="https://www.twitch.tv/rematch_gaming/chat?popout=" 
              frameborder="0" 
              scrolling="no">
            </iframe>
            <div class="streamSchedule">
              <img src="https://static-cdn.jtvnw.net/jtv_user_pictures/panel-165533043-image-34e42c8742531e4d-320-320.png">
            </div>
          </div>
        </div>
      </div>
      <div class="footer">
        <% include ../partials/footer %>
      </div>
    </div>
  </body>
</html>
